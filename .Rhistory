col<-"'col-sm-4'"
div<-"</div>"
a_link<-"<a href="
a<-"</a>"
a<-"</a>"
t_s<-"<table>"
tr_e<-"</tr>"
td_s<-"<td>"
t_e<-"</table>"
div_class_row<-paste(div_class,row,">",sep="")
div_class_col<-paste(div_class,col,">",sep="")
#Looping Through Each Category
for (c in categories){
#Category Subset
psub<-subset(p,Category==c)
#Vector for which picture to start new rows in output
